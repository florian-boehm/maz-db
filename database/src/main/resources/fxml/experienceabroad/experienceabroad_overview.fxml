<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import org.controlsfx.control.MaskerPane?>
<AnchorPane fx:id="tablePane" minHeight="0.0" minWidth="0.0" prefHeight="520.0" prefWidth="689.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.spiritaner.maz.controller.experienceabroad.ExperienceAbroadOverviewController">
	<children>
		<BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<bottom>
				<ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="30.0" prefWidth="746.0" BorderPane.alignment="CENTER">
					<items>
						<Button fx:id="removeButton" disable="true" mnemonicParsing="false" onAction="#remove" text="Entfernen" />
						<Button fx:id="editButton" disable="true" mnemonicParsing="false" onAction="#edit" text="Bearbeiten" />
						<Button fx:id="createButton" mnemonicParsing="false" onAction="#create" text="Neu" />
					</items>
				</ToolBar>
			</bottom>
			<center>
				<ScrollPane fitToHeight="true" fitToWidth="true" style="-fx-background-insets: 0;" BorderPane.alignment="CENTER">
					<content>
						<TableView fx:id="table" style="-fx-padding: 0; -fx-background-insets: 0px; -fx-background-color: #f4f4f4; -fx-border-width: 0 0 1 0; -fx-border-color: #c8c8c8;" tableMenuButtonVisible="true">
							<columns>
								<TableColumn fx:id="communityColumn" prefWidth="150.0" text="(Ordens-) Gemeinschaft" />
								<TableColumn fx:id="locationColumn" prefWidth="150.0" text="Ort" />
								<TableColumn fx:id="fromDateColumn" prefWidth="150.0" text="Von" />
								<TableColumn fx:id="toDateColumn" prefWidth="150.0" text="Bis" />
								<TableColumn fx:id="idColumn" minWidth="1.0" prefWidth="150.0" text="ID" />
							</columns>
							<opaqueInsets>
								<Insets />
							</opaqueInsets>
							<columnResizePolicy>
								<TableView fx:constant="UNCONSTRAINED_RESIZE_POLICY" />
							</columnResizePolicy>
						</TableView>
					</content>
				</ScrollPane>
			</center>
		</BorderPane>
		<MaskerPane fx:id="masker" visible="false" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
	</children>
</AnchorPane>
