<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import org.controlsfx.control.MaskerPane?>
<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="520.0" prefWidth="689.0"
            xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.spiritaner.maz.controller.residence.ResidenceOverviewController">
    <children>
        <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                    AnchorPane.topAnchor="0.0">
            <bottom>
                <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="30.0" prefWidth="746.0"
                         BorderPane.alignment="CENTER">
                    <items>
                        <Button mnemonicParsing="false" onAction="#remove" disable="true" fx:id="removeButton" text="Entfernen"/>
                        <Button mnemonicParsing="false" onAction="#edit" disable="true" fx:id="editButton" text="Bearbeiten"/>
                        <Button mnemonicParsing="false" onAction="#create" text="Neu"/>
                    </items>
                </ToolBar>
            </bottom>
            <center>
                <ScrollPane fitToHeight="true" fitToWidth="true" style="-fx-background-insets: 0;"
                            BorderPane.alignment="CENTER">
                    <content>
                        <TableView fx:id="table"
                                   style="-fx-padding: 0; -fx-background-insets: 0px; -fx-background-color: #f4f4f4; -fx-border-width: 0 0 1 0; -fx-border-color: #c8c8c8;"
                                   tableMenuButtonVisible="true">
                            <columns>
                                <TableColumn fx:id="residenceTypeColumn" prefWidth="83.0" text="Wohnortart"/>
                                <TableColumn fx:id="preferredAddressColumn" prefWidth="55.0" text="Primär"/>
                                <TableColumn fx:id="streetColumn" prefWidth="107.0" text="Straße"/>
                                <TableColumn fx:id="houseNumberColumn" prefWidth="117.0" text="Hausnummer"/>
                                <TableColumn fx:id="postCodeColumn" prefWidth="117.0" text="Postleitzahl"/>
                                <TableColumn fx:id="cityColumn" prefWidth="126.0" text="Stadt"/>
                                <TableColumn fx:id="stateColumn" prefWidth="126.0" text="Bundesland"/>
                                <TableColumn fx:id="countryColumn" prefWidth="116.0" text="Land"/>
                                <TableColumn fx:id="additionColumn" prefWidth="75.0" text="Adresszusatz"/>
                                <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID"/>
                            </columns>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <columnResizePolicy>
                                <TableView fx:constant="UNCONSTRAINED_RESIZE_POLICY"/>
                            </columnResizePolicy>
                        </TableView>
                    </content>
                </ScrollPane>
            </center>
        </BorderPane>
        <MaskerPane fx:id="masker" visible="false" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
    </children>
</AnchorPane>
